{
  "name": "board-server",
  "version": "1.0.0",
  "description": "An apollo-express server for the whiteboard app",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix src/**",
    "test": "jest",
    "start": "node src/server.js",
    "server": "nodemon src/server.js",
    "client": "npm run start --prefix ../client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/joshdcuneo/board.git"
  },
  "keywords": [
    "apollo",
    "graphql",
    "express",
    "react"
  ],
  "author": "Josh Cuneo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshdcuneo/board/issues"
  },
  "homepage": "https://github.com/joshdcuneo/board#readme",
  "dependencies": {
    "concurrently": "^3.6.1",

    "express": "^4.17.1",
    "mongoose": "^5.10.6",

    "express": "^4.16.3",
    "mongoose": "^5.10.1",

    "snyk": "^1.518.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.3"
  },
  "snyk": true
}
